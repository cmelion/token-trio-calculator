  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite build && cp public/mockServiceWorker.js dist/ && vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:bdd": "npx bddgen && npx playwright test",
    "test:bdd:generate": "npx bddgen",
    "test:bdd:run": "npx playwright test",
    "test:bdd:ci": "npm run test:bdd:generate && start-server-and-test \"storybook dev -p 6006 --ci --quiet --no-open\" http://localhost:6006 test:bdd:run",    "test:bdd:ui": "npx playwright test --ui",
    "test:bdd:debug": "npx playwright test --debug",
    "test:bdd:watch": "chokidar \"src/**/*.steps.ts\" \"src/**/*.feature\" -c \"npm run test:bdd:generate\"",
    "// Component Tests (Vitest)": "",
    "test:components": "vitest run --project components -t @component",
    "test:components:coverage": "vitest run --project components --coverage",
    "test:components:scenario": "vitest run --project components --testNamePattern",
    "test:components:watch": "vitest watch --project components"
